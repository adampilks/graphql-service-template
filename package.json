{
  "name": "graphql-service-template",
  "version": "1.0.0",
  "description": "A template for creating new GraphQL services",
  "main": "dist/index.js",
  "repository": "git@github.com:adampilks/graphql-service-template.git",
  "author": "Adam Pilkington",
  "private": true,
  "scripts": {
    "dev": "linkemon --inspect --exec babel-node src/index.ts --extensions '.ts'",
    "check-types": "tsc",
    "generate:types": "graphql-codegen --config codegen.yml",
    "lint": "eslint '**/*.ts'"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/node": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@graphql-codegen/cli": "1.2.1",
    "@graphql-codegen/typescript": "1.2.1",
    "@graphql-codegen/typescript-resolvers": "1.2.1",
    "@types/graphql": "14.2.0",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "babel-plugin-module-resolver": "3.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-eslint-comments": "3.1.1",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-no-only-tests": "^2.3.1",
    "eslint-plugin-no-switch-statements": "1.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "linkemon": "^0.0.2",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "typescript": "3.4.5",
    "utility-types": "^3.7.0"
  },
  "dependencies": {
    "apollo-datasource-rest": "0.5.0",
    "apollo-errors": "1.9.0",
    "apollo-resolvers": "1.4.1",
    "apollo-server": "2.6.2",
    "graphql": "14.3.1"
  }
}
